<Page x:Class="TeamsContactsGroupCreator.SignIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        Title="SignIn">
    <WrapPanel Orientation="Vertical" HorizontalAlignment="Center"  VerticalAlignment="Center">
        <Label FontSize="30" HorizontalContentAlignment="Center" Margin="0,0,0,30">Hey There!</Label>
        <Label FontSize="15">Please login into Microsoft Teams through GoogleChrome browser</Label>
        <Button Name="SignInButton" Style="{DynamicResource MaterialDesignRaisedDarkButton}" Margin="10" Click="SignIn_Button_Click">Perform Check?</Button>
        <TextBlock Name="ResultText"></TextBlock>
    </WrapPanel>
</Page>
